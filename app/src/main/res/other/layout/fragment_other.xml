<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="match_parent">    <Button        android:id="@+id/btn_webview"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="WebView"        android:textAllCaps="false"        app:layout_constraintBottom_toTopOf="@id/btn_call_phone"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <Button        android:id="@+id/btn_call_phone"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="Call Phone"        android:textAllCaps="false"        app:layout_constraintBottom_toTopOf="@id/btn_install_other_apk"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@id/btn_webview" />    <Button        android:id="@+id/btn_install_other_apk"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="Install Other Apk"        android:textAllCaps="false"        app:layout_constraintBottom_toTopOf="@id/btn_rx_android"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@id/btn_call_phone" />    <Button        android:id="@+id/btn_rx_android"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="RxAndroid"        android:textAllCaps="false"        app:layout_constraintBottom_toTopOf="@id/tv_stroke"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@id/btn_install_other_apk" />    <indi.toaok.androiddemo.module.comment.widget.StrokeTextView        android:id="@+id/tv_stroke"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="@string/app_name"        android:textColor="@color/colorRed"        app:isDrawStroke="false"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintLeft_toLeftOf="parent"        app:layout_constraintRight_toRightOf="parent"        app:layout_constraintTop_toBottomOf="@id/btn_rx_android"        app:strokeColor="@color/colorBlack"        app:strokeSize="@dimen/siez_2" /></androidx.constraintlayout.widget.ConstraintLayout>