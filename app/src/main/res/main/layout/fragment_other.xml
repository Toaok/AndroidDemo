<?xml version="1.0" encoding="utf-8"?><android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="match_parent">    <Button        android:id="@+id/tv_webview"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="WebView"        android:textAllCaps="false"        app:layout_constraintBottom_toBottomOf="@id/tv_call_phone"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <Button        android:id="@+id/tv_call_phone"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="Call Phone"        android:textAllCaps="false"        app:layout_constraintBottom_toTopOf="@id/tv_install_other_apk"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@id/tv_webview" />    <Button        android:id="@+id/tv_install_other_apk"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="Install Other Apk"        android:textAllCaps="false"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@id/tv_call_phone" /></android.support.constraint.ConstraintLayout>