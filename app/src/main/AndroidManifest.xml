<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"    package="com.toaok.study">    <!--网络权限-->    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />    <uses-permission android:name="android.permission.INTERNET" />    <!--拨打电话权限-->    <uses-permission android:name="android.permission.CALL_PHONE" />    <!--呼出电话监听-->    <uses-permission android:name="android.permission.PROCESS_OUTGOING_CALLS" />    <!--电话状态权限-->    <uses-permission android:name="android.permission.READ_PHONE_STATE" />    <application        android:name=".App"        android:allowBackup="true"        android:icon="@mipmap/ic_launcher"        android:label="@string/app_name"        android:supportsRtl="true"        android:theme="@style/AppTheme">        <activity android:name=".module.home.activity.SplashActivity">            <intent-filter>                <action android:name="android.intent.action.MAIN" />                <category android:name="android.intent.category.LAUNCHER" />            </intent-filter>        </activity>        <activity android:name=".module.home.activity.MainActivity" />        <activity android:name=".module.login.activity.LoginActivity" />    </application></manifest>