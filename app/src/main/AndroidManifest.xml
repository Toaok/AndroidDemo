<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"    package="com.toaok.study">    <!--wifi状态权限-->    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />    <!--网络权限-->    <uses-permission android:name="android.permission.INTERNET" />    <!--拨打电话权限-->    <uses-permission android:name="android.permission.CALL_PHONE" />    <!--呼出电话监听-->    <uses-permission android:name="android.permission.PROCESS_OUTGOING_CALLS" />    <!--电话状态权限-->    <uses-permission android:name="android.permission.READ_PHONE_STATE" />    <!--读取SD卡中文件-->    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />    <!--写入数据到SD卡-->    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />    <application        android:name=".App"        android:allowBackup="true"        android:icon="@mipmap/ic_launcher"        android:label="@string/app_name"        android:supportsRtl="true"        android:theme="@style/AppTheme">        <activity            android:name=".module.home.activity.SplashActivity"            android:theme="@style/AppTheme.Transparent">            <intent-filter>                <action android:name="android.intent.action.MAIN" />                <category android:name="android.intent.category.LAUNCHER" />            </intent-filter>        </activity>        <activity            android:name=".module.home.activity.MainActivity"            android:launchMode="singleTask" />        <activity            android:name=".module.login.activity.LoginActivity"            android:launchMode="singleTask" />        <!--启动一个新的进程来执行WebActivity-->        <activity            android:name=".module.other.activity.WebActivity"            android:launchMode="singleTask"            android:theme="@style/AppTheme" />    </application></manifest>