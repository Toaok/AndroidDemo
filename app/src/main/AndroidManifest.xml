<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"    package="indi.toaok">    <!--允许程序打开网络套接字-->    <uses-permission android:name="android.permission.INTERNET" />    <!--允许程序设置内置sd卡的写权限-->    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />    <!--允许程序设置内置sd卡的读权限-->    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />    <!--允许程序获取网络状态-->    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />    <!--允许程序访问WiFi网络信息-->    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />    <!--允许程序读写手机状态和身份-->    <uses-permission android:name="android.permission.READ_PHONE_STATE" />    <!--允许程序访问CellID或WiFi热点来获取粗略的位置-->    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />    <!--允许程序拨打电话-->    <uses-permission android:name="android.permission.CALL_PHONE" />    <!--允许程序监听呼出电话-->    <uses-permission android:name="android.permission.PROCESS_OUTGOING_CALLS" />    <!--允许程序安装安装包-->    <uses-permission android:name="android.permission.REQUEST_INSTALL_PACKAGES" />    <application        android:name=".App"        android:allowBackup="true"        android:icon="@mipmap/ic_toaok_launcher"        android:label="@string/app_name"        android:theme="@style/AppTheme">        <!--Provider Start-->        <provider            android:name="android.support.v4.content.FileProvider"            android:authorities="${applicationId}.fileProvider"            android:exported="false"            android:grantUriPermissions="true">            <meta-data                android:name="android.support.FILE_PROVIDER_PATHS"                android:resource="@xml/provider_paths" />        </provider>        <!--Provider End-->        <!--Activity Start-->        <activity            android:name=".module.home.activity.SplashActivity"            android:theme="@style/AppTheme.Transparent">            <intent-filter>                <action android:name="android.intent.action.MAIN" />                <category android:name="android.intent.category.LAUNCHER" />            </intent-filter>        </activity>        <activity            android:name=".module.home.activity.MainActivity"            android:configChanges="screenSize|keyboardHidden|orientation"            android:launchMode="singleTop" />        <activity            android:name=".module.login.activity.LoginActivity"            android:configChanges="screenSize|keyboardHidden|orientation"            android:launchMode="singleTop" />        <!--WebActivity Start-->        <!--启动一个新的进程来执行,返回时直接杀死进程-->        <activity            android:name=".module.other.activity.WebActivity"            android:launchMode="singleTop"            android:process=".webview"            android:theme="@style/AppTheme" />        <!--WebActivity End-->        <!--Activity End-->        <!--高德app key Start-->        <meta-data            android:name="com.amap.api.v2.apikey"            android:value="${GAODE_KEY}" />        <service android:name="com.amap.api.location.APSService" />        <!--高德 End-->    </application></manifest>